<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @product.title %>
</p>
<p>

  <%= image_tag @product.image_url %>
</p>

<p>
  <strong>Description:</strong>
  <%= @product.description %>
</p>


